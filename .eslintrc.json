{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base",
        "plugin:cypress/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
      "jest"
    ],
    "rules": {
      "import/extensions": [0, { "js": "always" }]
    }
}
